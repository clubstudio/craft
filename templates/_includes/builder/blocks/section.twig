{% embed '_includes/builder/section' with { block: block, key: key } only %}
    {% block content %}
        {% embed '_includes/builder/container' with { block: block, key: key } only %}
            {% block content %}
                {% include '_includes/builder/builder' with {
                    context: 'section',
                    blocks: block.contentBlock.eagerly().all(),
                    key: [key, block.maximumWidth.value]|join(':'),
                } %}
            {% endblock %}
        {% endembed %}
    {% endblock %}
{% endembed %}
